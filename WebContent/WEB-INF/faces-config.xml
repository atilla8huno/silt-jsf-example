<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.1" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">

	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>
	
	<application>
		<system-event-listener>
			<system-event-listener-class>br.gov.go.saude.silt.util.ViewScopeCallbackRegistrer</system-event-listener-class>
			<system-event-class>javax.faces.event.PostConstructViewMapEvent</system-event-class>
			<source-class>javax.faces.component.UIViewRoot</source-class>
		</system-event-listener>
	</application>
	<application>
		<system-event-listener>
			<system-event-listener-class>br.gov.go.saude.silt.util.ViewScopeCallbackRegistrer</system-event-listener-class>
			<system-event-class>javax.faces.event.PreDestroyViewMapEvent</system-event-class>
			<source-class>javax.faces.component.UIViewRoot</source-class>
		</system-event-listener>
	</application>

	<converter>
		<converter-id>cep</converter-id>
		<converter-class>br.gov.go.saude.arquitetura.controle.conversor.CEP</converter-class>
	</converter>
	<converter>
		<converter-id>texto</converter-id>
		<converter-class>br.gov.go.saude.arquitetura.controle.conversor.Texto</converter-class>
	</converter>
	<converter>
		<converter-id>trim</converter-id>
		<converter-class>br.gov.go.saude.arquitetura.controle.conversor.Trim</converter-class>
	</converter>
	<converter>
		<converter-id>numeroLongo</converter-id>
		<converter-class>br.gov.go.saude.arquitetura.controle.conversor.NumeroLongo</converter-class>
	</converter>
	<converter>
		<converter-id>numeroInteiro</converter-id>
		<converter-class>br.gov.go.saude.arquitetura.controle.conversor.NumeroInteiro</converter-class>
	</converter>
	<converter>
		<converter-id>cpf</converter-id>
		<converter-class>br.gov.go.saude.arquitetura.controle.conversor.CPF</converter-class>
	</converter>
	<converter>
		<converter-id>cnpj</converter-id>
		<converter-class>br.gov.go.saude.arquitetura.controle.conversor.CNPJ</converter-class>
	</converter>
	<converter>
		<converter-id>inscricaoMunicipal</converter-id>
		<converter-class>br.gov.go.saude.arquitetura.controle.conversor.InscricaoMunicipal</converter-class>
	</converter>
	<converter>
		<converter-id>inscricaoEstadual</converter-id>
		<converter-class>br.gov.go.saude.arquitetura.controle.conversor.InscricaoEstadual</converter-class>
		<attribute>
			<attribute-name>inscricaoEstadualestado</attribute-name>
			<attribute-class>java.lang.String</attribute-class>
		</attribute>
	</converter>
	<converter>
		<converter-id>lista</converter-id>
		<converter-class>br.gov.go.saude.arquitetura.controle.conversor.Lista</converter-class>
	</converter>
	<managed-bean>
		<managed-bean-name>cEstrutura</managed-bean-name>
		<managed-bean-class>br.gov.go.saude.arquitetura.controle.CEstrutura</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<navigation-case>
			<from-outcome>inicio</from-outcome>
			<to-view-id>/estrutura/principal.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>erroAutenticacao</from-outcome>
			<to-view-id>/estrutura/erro_autenticacao.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>erro</from-outcome>
			<to-view-id>/estrutura/erro.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<validator>
		<validator-id>cpf</validator-id>
		<validator-class>br.gov.go.saude.arquitetura.controle.validador.CPF</validator-class>
	</validator>
	<validator>
		<validator-id>cnpj</validator-id>
		<validator-class>br.gov.go.saude.arquitetura.controle.validador.CNPJ</validator-class>
	</validator>
	<validator>
		<validator-id>email</validator-id>
		<validator-class>br.gov.go.saude.arquitetura.controle.validador.Email</validator-class>
	</validator>
</faces-config>
